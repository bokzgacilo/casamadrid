        "default": "#FFFFFF"
      },
      {
        "type": "color",
        "label": "Right Text Color",
        "id": "right_text_color",
        "default": "#FFFFFF"
      },
      {
        "type": "header",
        "content": "Item Colors"
      },
      {
        "type": "color",
