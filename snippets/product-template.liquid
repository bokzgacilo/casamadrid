                        const shippingDate = new Date(today.getTime() + (shippingDays * 24 * 60 * 60 * 1000)); // Add shippingDays to the current date

                        const options = { weekday: 'long', day: 'numeric', month: 'long' };
                        return shippingDate.toLocaleDateString('es-ES', options);
                      }

