            <style>
              .custom-collapsible-section-wrapper {
                max-width: 100%;
                margin: 2rem auto;
              }

              .collapsible-items-container {
                overflow: hidden;
              }

              .collapsible-item:last-child {
                border-bottom: none;
              }

              .custom-collapsible-header {
                width: 100%;
                display: flex;
                justify-content: space-between;
                align-items: center;
                font-weight: 700;
                padding: 14px 20px;
                color: #000000;
                border: 1px solid #e2e2e2;
                border-bottom: 0;
                border-left: 0;
                border-right: 0;
                font-size: 14px;
                background-color: transparent;
                cursor: pointer;
                text-align: left;
              }
              .collapsible-icon {
                display: flex;
                align-items: center;
                justify-content: center;
                width: 24px;
                height: 24px;
                transition: transform 0.3s ease;
              }

              .collapsible-icon .icon-chevron-down {
                display: none;
                /* Hide minus icon by default */
              }

              .collapsible-header.active .collapsible-icon .icon-chevron-up {
                display: none;
                /* Hide plus when active */
              }

              .collapsible-header.active .collapsible-icon .icon-chevron-down {
                display: block;
                /* Show minus when active */
              }

              .collapsible-icon svg {
                width: 18px;
                height: 18px;
                fill: currentColor;
              }

              .custom-collapsible-content {
                max-height: 0;
                overflow: hidden;
                background-color: transparent;
                padding: 0 1rem;
              }

              .custom-collapsible-content.open {
                max-height: 500px;
                overflow: auto;
                padding: 1rem;
              }

              .custom-collapsible-content p {
                margin-bottom: 1rem;
                line-height: 1.6;
                color: #000000;
              }

              .collapsible-content p:last-child {
                margin-bottom: 0;
              }
            </style>
            <script>
              $(document).ready(function() {
              const $collapsibleHeaders = $('.custom-collapsible-header');

              $collapsibleHeaders.on('click', function() {
                const $this = $(this); // Cache the jQuery object for the clicked header
                const $content = $this.next('.custom-collapsible-content'); // Get the next sibling with class .collapsible-content
                const isActive = $this.hasClass('active');

                $this.toggleClass('active');
                $content.toggleClass('open');

                $this.attr('aria-expanded', !isActive);
                $content.attr('aria-hidden', isActive);

                $collapsibleHeaders.not($this).each(function() {
                  const $otherHeader = $(this);
                  const $otherContent = $otherHeader.next('.custom-collapsible-content');
                  if ($otherHeader.hasClass('active')) {
                    $otherHeader.removeClass('active');
                    $otherContent.removeClass('open');
                    $otherHeader.attr('aria-expanded', 'false');
                    $otherContent.attr('aria-hidden', 'true');
                  }
                });
                
              });
                          });
            </script>
